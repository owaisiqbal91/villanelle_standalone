{
  "name": "electron-react-villanelle",
  "productName": "Villanelle",
  "version": "1.0.0",
  "description": "Villanelle story authoring tool",
  "main": "./src/index.ts",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "tslint --project tsconfig.json --type-check --force",
    "antlr4": "antlr4ts -o src/parsing -visitor -no-listener src/parsing/VillanelleGrammar.g4",
    "pack": "electron-builder --dir",
    "dist": "electron-builder"
  },
  "keywords": [],
  "author": "owais",
  "license": "MIT",
  "config": {
    "forge": {
      "make_targets": {
        "win32": [
          "zip"
        ],
        "darwin": [
          "zip"
        ],
        "linux": [
          "deb",
          "rpm"
        ]
      },
      "electronPackagerConfig": {
        "packageManager": "npm"
      },
      "electronWinstallerConfig": {
        "name": "electron_react_villanelle"
      },
      "electronInstallerDebian": {},
      "electronInstallerRedhat": {},
      "github_repository": {
        "owner": "",
        "name": ""
      },
      "windowsStoreConfig": {
        "packageName": "",
        "name": "electronreactvillanelle"
      }
    }
  },
  "build": {
    "appId": "Villanelle"
  },
  "dependencies": {
    "@blueprintjs/core": "^3.13.0",
    "@types/electron-devtools-installer": "^2.2.0",
    "@types/js-yaml": "^3.12.0",
    "@types/react": "^16.7.18",
    "@types/react-dom": "^16.0.11",
    "ajv": "^6.8.1",
    "ajv-errors": "^1.0.1",
    "ajv-keywords": "^3.3.0",
    "antlr4": "^4.7.2",
    "antlr4ts": "^0.5.0-alpha.1",
    "brace": "^0.11.1",
    "css-loader": "^2.1.1",
    "electron-compile": "^6.4.4",
    "electron-devtools-installer": "^2.2.4",
    "electron-prebuilt-compile": "^4.0.0",
    "electron-squirrel-startup": "^1.0.0",
    "html-webpack-plugin": "^3.2.0",
    "js-yaml": "^3.12.1",
    "jsonschema": "^1.2.4",
    "mousetrap": "^1.6.3",
    "node": "^8.10.0",
    "node-win-x64": "^11.14.0",
    "react": "^16.8.1",
    "react-ace": "^6.5.0",
    "react-dom": "^16.8.1",
    "react-hot-loader": "^3.1.3",
    "react-mosaic-component": "^2.0.2",
    "react-rnd": "^9.1.1",
    "tslib": "^1.9.3",
    "typescript": "^2.9.2",
    "typescript-collections": "^1.3.2"
  },
  "devDependencies": {
    "antlr4ts-cli": "^0.5.0-alpha.1",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "electron": "^4.1.4",
    "electron-builder": "^20.39.0",
    "electron-forge": "^5.2.3",
    "electron-packager": "^13.1.1",
    "tslint": "^5.12.0",
    "webpack": "^4.29.3",
    "webpack-cli": "^3.2.3"
  }
}
